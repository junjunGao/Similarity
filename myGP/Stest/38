#include <stdio.h>
#include <stdlib.h>
int rn(int x)
{   if((0==x%4)&&(0!=x%100))
return 1;
  if(0==x%400)
  return 1;
  return 0;    }

int main()
{
    int a,b,c,i,n;
    scanf("%d %d %d",&a,&b,&c);
    i=rn(a);
    if(i==0)
    {  if (b==1)
        { n=c;
        }
        if (b==2)
        { n=c+31;
        }
        if (b==3)
        { n=c+31+28;
        }if (b==4)
        { n=c+62+28;
        }if (b==5)
        { n=c+90+30;
        }if (b==6)
        { n=c+120+31;
        }if (b==7)
        { n=c+151+30;
        }if (b==8)
        { n=c+181+31;
        }if (b==9)
        { n=c+181+31+31;
        }if (b==10)
        { n=c+181+62+30;
        }if (b==11)
        { n=c+181+62+31+30;
        }if (b==12)
        { n=c+181+62+61+30;
        }
        }
        if(1==i)
        {if (b==1)
        { n=c;
        }
        if (b==2)
        { n=c+31;
        }
        if (b==3)
        { n=c+31+29;
        }if (b==4)
        { n=c+62+29;
        }if (b==5)
        { n=c+91+30;
        }if (b==6)
        { n=c+121+31;
        }if (b==7)
        { n=c+151+31;
        }if (b==8)
        { n=c+182+31;
        }if (b==9)
        { n=c+182+31+31;
        }if (b==10)
        { n=c+182+62+30;
        }if (b==11)
        { n=c+182+62+31+30;
        }if (b==12)
        { n=c+182+62+61+30;
        }
            }
            printf ("%d",n);
    return 0;
}
