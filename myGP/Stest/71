#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c,n;
    scanf("%d %d %d",&a,&b,&c);
    if((a%100!=0&&a%4==0)||a%400==0)
    {
        if(b==1)
        n=c;
        else if(b==2)
        n=31+c;
        else if(b==3)
        n=29+31+c;
        else if(b==4)
        n=31+29+31+c;
        else if(b==5)
        n=30+31+29+31+c;
        else if(b==6)
        n=31+30+31+29+31+c;
        else if(b==7)
        n=30+31+30+31+29+31+c;
        else if(b==8)
        n=31+30+31+30+31+29+31+c;
        else if(b==9)
        n=31+31+30+31+30+31+29+31+c;
        else if(b==10)
        n=30+31+31+30+31+30+31+29+31+c;
        else if(b==11)
        n=31+30+31+31+30+31+30+31+29+31+c;
        else if(b==1)
        n=30+31+30+31+31+30+31+30+31+29+31+c;
    }

    else if((a%100==0&&a%400!=0)||a%4!=0)
    {
        if(b==1)
        n=c;
        else if(b==2)
        n=31+c;
        else if(b==3)
        n=28+31+c;
        else if(b==4)
        n=31+28+31+c;
        else if(b==5)
        n=30+31+28+31+c;
        else if(b==6)
        n=31+30+31+28+31+c;
        else if(b==7)
        n=30+31+30+31+28+31+c;
        else if(b==8)
        n=31+30+31+30+31+28+31+c;
        else if(b==9)
        n=31+31+30+31+30+31+28+31+c;
        else if(b==10)
        n=30+31+31+30+31+30+31+28+31+c;
        else if(b==11)
        n=31+30+31+31+30+31+30+31+28+31+c;
        else if(b==1)
        n=30+31+30+31+31+30+31+30+31+28+31+c;
    }

    printf("%d",n);
    return 0;
}
