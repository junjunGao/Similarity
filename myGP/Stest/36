#include <stdio.h>
#include <stdlib.h>

int main()
{
    int y,m,d,num;
    scanf("%d %d %d",&y,&m,&d);
    if( (y%4==0&&y%100!=0) || (y%400==0) )
    {
        if(m==1)
        num=d;
        if(m==2)
        num=31+d;
        if(m==3)
        num=31+29+d;
        if(m==4)
        num=31+29+31+d;
        if(m==5)
        num=31+29+31+30+d;
        if(m==6)
        num=31+29+31+30+31+d;
        if(m==7)
        num=31+29+31+30+31+30+d;
        if(m==8)
        num=31+29+31+30+31+30+31+d;
        if(m==9)
        num=31+29+31+30+31+30+31+31+d;
        if(m==10)
        num=31+29+31+30+31+30+31+31+30+d;
        if(m==11)
        num=31+29+31+30+31+30+31+31+30+31+d;
        if(m==12)
        num=31+29+31+30+31+30+31+31+30+31+30+d;
    }
    else
    {
        if(m==1)
        num=d;
        if(m==2)
        num=31+d;
        if(m==3)
        num=31+29+d-1;
        if(m==4)
        num=31+29+31+d-1;
        if(m==5)
        num=31+29+31+30+d-1;
        if(m==6)
        num=31+29+31+30+31+d-1;
        if(m==7)
        num=31+29+31+30+31+30+d-1;
        if(m==8)
        num=31+29+31+30+31+30+31+d-1;
        if(m==9)
        num=31+29+31+30+31+30+31+31+d-1;
        if(m==10)
        num=31+29+31+30+31+30+31+31+30+d-1;
        if(m==11)
        num=31+29+31+30+31+30+31+31+30+31+d-1;
        if(m==12)
        num=31+29+31+30+31+30+31+31+30+31+30+d-1;
    }
    printf("%d",num);
    return 0;
}
